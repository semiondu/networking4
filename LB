import socket

PORT = 80
HOST = ''
SERVER1 = 192.168.0.101
SERVER2 = 192.168.0.102
SERVER3 = 192.168.0.103

with socket.socket(socket.AF_INET, socket.SOCK_STREAM) as hostSocket:
    hostSocket.bind((HOST, PORT))
    hostSocket.listen(1)
    conn, addr = hostSocket.accept()
    with conn:
        print('Connected by', addr)
        while True:
            data = conn.recv(1024)
            if not data: break
            conn.sendall(data)
