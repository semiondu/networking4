mport socket

PORT = 80
HOST = ''


hostSocket = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
hostSocket.bind((HOST, PORT))
hostSocket.listen(1)
conn, addr = hostSocket.accept()
with conn:
    print('Connected by', addr)
    while True:
        data = conn.recv(1024)
        if not data: break
        conn.sendall(data)
